{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "amd": true,
    "node": true
  },
  "ecmaFeatures": {
    "modules": true,
    "jsx": true
  },
  "rules": {
    "semi": 1,
    "global-strict": 0,
    "quotes": [
      2,
      "single"
    ],
    "no-undef": 1,
    "key-spacing": 0,
    "no-trailing-spaces": 0,
    "eol-last": 0,
    "no-underscore-dangle": 0,
    "no-console": 0,
    "no-alert": 0
  }
}
